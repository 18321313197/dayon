<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--命名空间应该是对应接口的包名+接口名 -->
<mapper namespace="com.dayon.b2b2c.center.auth.mapper.UserDisableMapper">

	<resultMap id="UserDisable" type="com.dayon.b2b2c.api.auth.entity.UserDisable">
		<id property="id" column="c_id" />
		<result property="userId" column="c_user_id" />
		<result property="platformId" column="c_platform_id" />
		<result property="note" column="c_note" />
		<result property="startTime" column="c_start_time" />
		<result property="stopTime" column="c_stop_time" />
		<result property="createTime" column="c_create_time" />
		<result property="createBy" column="c_create_by" />
	</resultMap>


	<select id="find" resultMap="UserDisable">
		select * from t_qx_user_disable
	</select>
	<select id="get" resultMap="UserDisable">
		select * from t_qx_user_disable
		where c_id=#{id}
	</select>
	<insert id="add" parameterType="com.dayon.b2b2c.api.auth.entity.UserDisable">
		insert into t_qx_user_disable(
		c_id,
		c_user_id
		c_platform_id,
		c_note,
		c_start_time,
		c_stop_time,
		c_create_time,
		c_create_by
		)
		value
		(
		#{id},#{userId},#{platformId},#{note},#{startTime},#{stopTime},#{createTime},#{createBy}
		)
	</insert>

</mapper>